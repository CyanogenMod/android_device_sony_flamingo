#SONY misc
service tad_static /system/bin/tad_static /dev/block/platform/msm_sdcc.1/by-name/TA 0,16
    user root
    group root
    socket tad stream 0660 system system
    class core

#QCOM sensors
service sensors /system/bin/sensors.qcom
    class main
    user root
    group root

#MAC setup
service btnvtool /system/bin/btnvtool -I
    user root
    group root
    oneshot
